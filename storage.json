{
  "preferences": {},
  "staffContent": "<div class=\"fixed bottom-0 right-0 p-4 flex space-x-4 z-50\"> \n    <b>Text:</b>\n    <select id=\"text-color\" onchange=\"changeTextColor()\">\n<option value=\"default\" style=\"color: black;\">Default</option>\n<option value=\"green\" style=\"color: black;\">Green Text</option>\n<option value=\"red\" style=\"color: black;\">Red Text</option>\n<option value=\"golden\" style=\"color: black;\">Golden Text</option>\n<option value=\"rainbow\" style=\"color: black;\">Rainbow Text</option>\n    </select>\n\n    <b>Theme:</b>\n    <select id=\"theme\" onchange=\"changeTheme()\">\n        <option value=\"default\">Light Theme</option>\n        <option value=\"dark\">Dark Theme</option>\n    </select>\n</div>\n\n<script>\n    function loadUserPreferences() {\n      fetch('/api/preferences')\n        .then(response => response.json())\n        .then(prefs => {\n          if (prefs.textColor) {\n            document.getElementById('text-color').value = prefs.textColor;\n            changeTextColor();\n          }\n          if (prefs.theme) {\n            document.getElementById('theme').value = prefs.theme;\n            changeTheme();\n          }\n        })\n        .catch(err => console.error('Error loading preferences:', err));\n    }\n    document.addEventListener('DOMContentLoaded', loadUserPreferences);\n  \n    function changeTextColor() {\n      var textColor = document.getElementById(\"text-color\").value;\n      fetch('/api/preferences', {\n        method: 'POST',\n        headers: { 'Content-Type': 'application/json' },\n        body: JSON.stringify({ textColor: textColor })\n      });\n      // ... update classes as before ...\n    }\n  \n    function changeTheme() {\n      var theme = document.getElementById(\"theme\").value;\n      fetch('/api/preferences', {\n        method: 'POST',\n        headers: { 'Content-Type': 'application/json' },\n        body: JSON.stringify({ theme: theme })\n      });\n      // ... update classes as before ...\n      changeTextColor();\n    }\n  </script>\n  \n\n<style>\n    .green-text {\n        color: #34C759;\n    }\n    .green-text .text-gray-700 {\n        color: #34C759;\n    }\n    .green-text .text-gray-800 {\n        color: #34C759;\n    }\n    .green-text .text-gray-900 {\n        color: #34C759;\n    }\n\n    .red-text {\n        color: #FF3737;\n    }\n    .red-text .text-gray-700 {\n        color: #FF3737;\n    }\n    .red-text .text-gray-800 {\n        color: #FF3737;\n    }\n    .red-text .text-gray-900 {\n        color: #FF3737;\n    }\n\n    .golden-text {\n        color: #FFD700;\n    }\n    .golden-text .text-gray-700 {\n        color: #FFD700;\n    }\n    .golden-text .text-gray-800 {\n        color: #FFD700;\n    }\n    .golden-text .text-gray-900 {\n        color: #FFD700;\n    }\n    .rainbow-text {\n        animation: rainbow 2s infinite;\n    }\n\n    @keyframes rainbow {\n        0% {\n            color: #FF3737;\n        }\n        20% {\n            color: #FFD700;\n        }\n        40% {\n            color: #34C759;\n        }\n        60% {\n            color: #4B0082;\n        }\n        80% {\n            color: #8F0A1A;\n        }\n        100% {\n            color: #FF3737;\n        }\n    }\n</style>\n\n<style>\n    .light-mode {\n        background-color: #fff;\n        color: #000;\n    }\n    .light-mode .bg-white {\n        background-color: #fff;\n    }\n    .light-mode .text-white {\n        color: #000;\n    }\n    .light-mode .text-gray-700 {\n        color: #000;\n    }\n    .light-mode .text-gray-800 {\n        color: #000;\n    }\n    .light-mode .text-gray-900 {\n        color: #000;\n    }\n    .light-mode .bg-gray-100 {\n        background-color: #fff;\n    }\n    .light-mode .bg-gray-200 {\n        background-color: #fff;\n    }\n    .light-mode .bg-gray-800 {\n        background-color: #fff;\n    }\n    .light-mode .shadow-md {\n        box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);\n    }\n    \n.dark-mode {\n    background-color: #000;\n    color: var(--text-color);\n}\n.dark-mode .bg-white {\n    background-color: #000;\n}\n.dark-mode .text-white {\n    color: var(--text-color);\n}\n.dark-mode .text-gray-700 {\n    color: var(--text-color);\n}\n.dark-mode .text-gray-800 {\n    color: var(--text-color);\n}\n.dark-mode .text-gray-900 {\n    color: var(--text-color);\n}\n.dark-mode .bg-gray-100 {\n    background-color: #000;\n}\n.dark-mode .bg-gray-200 {\n    background-color: #000;\n}\n.dark-mode .bg-gray-800 {\n    background-color: #000;\n}\n.dark-mode .shadow-md {\n    box-shadow: 0 0 10px rgba(255, 255, 255, 0.5);\n}\n\n:root {\n    --text-color: #fff;\n}\n\n.green-text {\n    --text-color: #34C759;\n}\n\n.red-text {\n    --text-color: #FF3737;\n}\n\n.golden-text {\n    --text-color: #FFD700;\n}\n\n.rainbow-text {\n    animation: rainbow 2s infinite;\n}\n\n.light-mode .rainbow-text {\n    animation: rainbow 2s infinite;\n}\n\n@keyframes rainbow {\n    0% {\n        color: #FF3737;\n    }\n    20% {\n        color: #FFD700;\n    }\n    40% {\n        color: #34C759;\n    }\n    60% {\n        color: #4B0082;\n    }\n    80% {\n        color: #8F0A1A;\n    }\n    100% {\n        color: #FF3737;\n    }\n}\n\n</style>\n\n<div class=\"container mx-auto flex justify-between items-center px-4 py-8\" contenteditable=\"true\">\n<h1 class=\"text-3xl font-bold uppercase md:text-4xl\" style=\"background-image: url('NYCBanner.jpg'); background-size: 100% 100%; background-position: center; -webkit-background-clip: text; -webkit-text-fill-color: transparent;\">Dragon's Scales</h1>\n    <!-- Normal Navigation (Hidden on Mobile) -->\n    <nav class=\"hidden md:flex\">\n        <ul class=\"flex space-x-6 text-gray-700\">\n            <li><a href=\"index.html\" class=\"hover:text-gray-900\">Home</a></li>\n            <li><a href=\"about.html\" class=\"hover:text-gray-900\">About</a></li>\n            <li><a href=\"features.html\" class=\"hover:text-gray-900\">Features</a></li>\n            <li><a href=\"news.html\" class=\"hover:text-gray-900\">News</a></li>\n            <li><a href=\"staff.html\" class=\"hover:text-gray-900\">Staff</a></li>\n            <li><a href=\"gallery.html\" class=\"hover:text-gray-900\">Gallery</a></li>\n        </ul>\n    </nav>\n\n<div class=\"fixed bottom-7 left-0 p-4 flex space-x-4 z-50\" id=\"scroll-to-top\" style=\"display: block;\">\n    <p class=\"text-gray-700 text-sm hidden md:block\">Press 'E' to scroll back up</p>\n    <button class=\"bg-orange-500 hover:bg-orange-700 text-white font-bold py-2 px-4 rounded\" onclick=\"window.scrollTo({top: 0, behavior: 'smooth'});\">\n        <svg class=\"w-6 h-6\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\" xmlns=\"http://www.w3.org/2000/svg\">\n            <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M5 10l7-7m0 0l7 7m-7-7v18\"></path>\n        </svg>\n    </button>\n</div>\n\n<script>\n    window.addEventListener('scroll', function() {\n        var scrollPosition = window.scrollY;\n        if (scrollPosition > 200) {\n            document.getElementById('scroll-to-top').style.display = 'block';\n        } else {\n            document.getElementById('scroll-to-top').style.display = 'none';\n        }\n    });\n\n    document.addEventListener('keydown', function(event) {\n        if (event.key === 'e' || event.key === 'E') {\n            window.scrollTo({top: 0, behavior: 'smooth'});\n        }\n    });\n</script>\n\n    <!-- Burger Menu Icon (Only Visible on Mobile) -->\n    <button id=\"menu-btn\" class=\"md:hidden absolute top-4 right-4 text-gray-700 focus:outline-none\">\n        <svg class=\"w-8 h-8\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\" xmlns=\"http://www.w3.org/2000/svg\">\n            <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M4 6h16M4 12h16M4 18h16\"></path>\n        </svg>\n    </button>\n</div>\n\n<!-- Sidebar Menu (Hidden by Default) -->\n<div id=\"sidebar\" class=\"fixed top-0 left-0 w-64 h-full bg-white shadow-lg transform -translate-x-full transition-transform duration-300 md:hidden z-50\">\n<button id=\"close-btn\" class=\"absolute top-4 right-4 text-gray-700 focus:outline-none text-4xl px-2\">X</button>\n    <ul class=\"mt-12 space-y-6 text-gray-700 text-lg px-6\">\n        <li><a href=\"index.html\" class=\"block hover:text-gray-900\">Home</a></li>\n        <li><a href=\"about.html\" class=\"block hover:text-gray-900\">About</a></li>\n        <li><a href=\"features.html\" class=\"block hover:text-gray-900\">Features</a></li>\n        <li><a href=\"news.html\" class=\"block hover:text-gray-900\">News</a></li>\n        <li><a href=\"staff.html\" class=\"block hover:text-gray-900\">Staff</a></li>\n        <li><a href=\"gallery.html\" class=\"block hover:text-gray-900\">Gallery</a></li>\n    </ul>\n</div>\n\n<!-- JavaScript to Handle Menu Toggle -->\n<script>\n    const menuBtn = document.getElementById('menu-btn');\n    const sidebar = document.getElementById('sidebar');\n    const closeBtn = document.getElementById('close-btn');\n\n    menuBtn.addEventListener('click', () => {\n        sidebar.classList.remove('-translate-x-full');\n    });\n\n    closeBtn.addEventListener('click', () => {\n        sidebar.classList.add('-translate-x-full');\n    });\n</script>\n\n<!-- CSS to Hide Normal Menu and Show Burger on Mobile -->\n<style>\n    @media only screen and (max-width: 768px) {\n        nav { display: none; } /* Hide normal menu */\n    }\n</style>\n\n\n    <!-- Main Content -->\n<section class=\"bg-gray-100 py-24\" style=\"background-image: url('NYCBanner.jpg'); background-size: cover; background-position: center; filter: brightness(1); image-rendering: crisp-edges; image-rendering: -moz-crisp-edges; image-rendering: -webkit-optimize-contrast; image-rendering: optimize-contrast;\">\n    <div class=\"container mx-auto px-4\" contenteditable=\"true\">\n        <h2 class=\"text-5xl font-bold uppercase text-center mb-4 text-white glow-bright-white\" style=\"text-shadow: 0 0 10px #fff;\">Mock Trial</h2>\n        <p class=\"text-lg text-white text-center mb-8 glow-bright-white\" style=\"text-shadow: 0 0 10px #fff;\">\"Where Future Attorneys Are Made.\"</p>\n        <div class=\"flex justify-center\">\n            <button class=\"bg-orange-500 hover:bg-orange-700 text-white font-bold py-2 px-4 rounded glow-bright-yellow\" onclick=\"window.location.href='about.html'\">Learn More</button>\n        </div>\n    </div>\n</section>\n\n        <!-- Hero Section -->\n<section class=\"bg-gray-100 py-12\">\n    <div class=\"container mx-auto px-4\" contenteditable=\"true\">\n        <h2 class=\"text-5xl font-bold uppercase text-center mb-4\">What We Do</h2>\n        <div class=\"flex justify-center\">\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Case Preparation</h3>\n                    <p class=\"text-gray-700\">We analyze legal cases, develop arguments, and craft compelling defenses and prosecutions.</p>\n                </div>\n            </div>\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Courtroom Simulations</h3>\n                    <p class=\"text-gray-700\">Compete in mock trials, acting as attorneys and witnesses in real legal situations.</p>\n                </div>\n            </div>\n\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Legal Strategy &amp; Public Speaking</h3>\n                    <p class=\"text-gray-700\">Learn to think on your feet, argue persuasively, and master courtroom procedures.</p>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>\n\n<section class=\"bg-gray-100 py-12\">\n    <div class=\"container mx-auto px-4\" contenteditable=\"true\">\n        <h2 class=\"text-5xl font-bold uppercase text-center mb-4\">Why Join Mock Trial</h2>\n        <div class=\"bg-white rounded shadow-md p-4 mx-auto w-full md:w-1/2 xl:w-1/3 animate-pulse\">\n            <h3 class=\"text-lg font-bold mb-2\">Develop Essential Skills, Gain Practical Experience, and Build Your Network</h3>\n            <form>\n                <ul>\n                    <li>\n                        <input type=\"checkbox\" id=\"confidence\" name=\"confidence\" checked=\"\" style=\"accent-color: blue;\">\n                        <label for=\"confidence\"><b>Confidence</b> – Stand before a judge and jury with poise.</label>\n                    </li>\n                    <li>\n                        <input type=\"checkbox\" id=\"critical-thinking\" name=\"critical-thinking\" checked=\"\" style=\"accent-color: blue;\">\n                        <label for=\"critical-thinking\"><b>Critical Thinking</b> – Break down complex arguments and respond under pressure.</label>\n                    </li>\n                    <li>\n                        <input type=\"checkbox\" id=\"collaboration\" name=\"collaboration\" checked=\"\" style=\"accent-color: blue;\">\n                        <label for=\"collaboration\"><b>Collaboration</b> – Work as a team to build a compelling case.</label>\n                    </li>\n                    <li>\n                        <input type=\"checkbox\" id=\"college-career-prep\" name=\"college-career-prep\" checked=\"\" style=\"accent-color: blue;\">\n                        <label for=\"college-career-prep\"><b>College &amp; Career Prep</b> – Gain experience that sets you apart in law, politics, and beyond.</label>\n                    </li>\n                </ul>\n            </form>\n        </div>\n    </div>\n</section>\n\n\n<style>\n    .animate-pulse {\n        animation: pulse 2s infinite;\n    }\n\n    @keyframes pulse {\n        0% {\n            transform: scale(1);\n        }\n        50% {\n            transform: scale(1.05);\n        }\n        100% {\n            transform: scale(1);\n        }\n    }\n</style>\n\n<section class=\"bg-gray-100 py-12\">\n    <div class=\"container mx-auto px-4\" contenteditable=\"true\">\n        <h2 class=\"text-5xl font-bold uppercase text-center mb-4\">Competitions &amp; Achievements</h2>\n        <div class=\"flex justify-center\">\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Tournament Wins</h3>\n                    <ul>\n                        <li class=\"text-gray-700\">2022 Regional Championship</li>\n                        <li class=\"text-gray-700\">2023 State Championship</li>\n                        <li class=\"text-gray-700\">2024 National Championship</li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Empty Spot</h3>\n                    <ul>\n                        <li class=\"text-gray-700\">Empty</li>\n                        <li class=\"text-gray-700\">Empty</li>\n                        <li class=\"text-gray-700\">Empty</li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Team Recognition</h3>\n                    <ul>\n                        <li class=\"text-gray-700\">2022 Regional Championship - 2nd Place</li>\n                        <li class=\"text-gray-700\">2023 State Championship - 3rd Place</li>\n                        <li class=\"text-gray-700\">2024 National Championship - 1st Place</li>\n                    </ul>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>\n\n<section class=\"bg-gray-100 py-12\" style=\"animation: none;\">\n    <div class=\"container mx-auto px-4\" contenteditable=\"true\">\n        <h2 class=\"text-5xl font-bold uppercase text-center mb-4\">Join Us</h2>\n        <div class=\"flex justify-center\">\n            <div class=\"bg-white rounded shadow-md p-4 mx-auto w-full md:w-1/2 xl:w-1/3 text-center animate-fade-in\" style=\"animation: pulse 2s infinite;\">\n                <h3 class=\"text-lg font-bold mb-2 text-golden\">Become a Part of Our Competitive Team</h3>\n                <p class=\"text-gray-700\">Are you ready to argue, defend, and win? The JDHS Mock Trial Team is always looking for passionate students who want to sharpen their legal skills. Contact us to learn more!</p>\n                <div class=\"flex justify-center mt-8\">\n                    <button class=\"bg-orange-500 hover:bg-orange-700 text-white font-bold py-2 px-4 rounded glow-bright-golden\" onclick=\"window.location.href='contact.html'\">Get in Touch</button>\n                    <button class=\"bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded ml-4 glow-bright-golden\" onclick=\"document.getElementById('availability').classList.toggle('hidden')\">See availability first!</button>\n                </div>\n            </div>\n            <div class=\"bg-white rounded shadow-md p-4 mx-auto w-full md:w-1/2 xl:w-1/3 text-center ml-4 animate-fade-in\" style=\"animation: pulse 2s infinite;\">\n                <h3 class=\"text-lg font-bold mb-2 text-golden\">Join Our Development Team</h3>\n                <p class=\"text-gray-700\">Would you like to prepare to become a part of the Competitive Team? This is the perfect spot to learn and succeed, Hands on! Contact us to learn more!</p>\n                <div class=\"flex justify-center mt-8\">\n                    <button class=\"bg-orange-500 hover:bg-orange-700 text-white font-bold py-2 px-4 rounded glow-bright-golden\" onclick=\"window.location.href='contact.html'\">Get in Touch</button>\n                    <button class=\"bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded ml-4 glow-bright-golden\" onclick=\"document.getElementById('development-availability').classList.toggle('hidden')\">See availability first!</button>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>\n\n<style>\n    .animate-fade-in {\n        animation: fadeIn 0.5s;\n    }\n\n    @keyframes fadeIn {\n        from {\n            opacity: 0;\n            transform: scale(0.5);\n        }\n        to {\n            opacity: 1;\n            transform: scale(1);\n        }\n    }\n</style>\n\n<style>\n    .pulse {\n        animation: pulse 2s infinite;\n    }\n\n    @keyframes pulse {\n        0% {\n            transform: scale(1);\n        }\n        50% {\n            transform: scale(1.05);\n        }\n        100% {\n            transform: scale(1);\n        }\n    }\n</style>\n\n<style>\n    .text-golden {\n        color: #FFD700;\n    }\n    .glow-bright-golden {\n        box-shadow: 0 0 10px #FFD700;\n    }\n</style>\n\n<div id=\"development-availability\" class=\"fixed top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2 bg-white rounded shadow-md p-4 w-1/2 hidden animate-fade-in\">\n    <h3 class=\"text-lg font-bold mb-2\">Development Team Availability</h3>\n    <p class=\"text-gray-700\">Development Team is currently open to new Participants! Please contact us if interested!</p>\n    <button class=\"bg-orange-500 hover:bg-orange-700 text-white font-bold py-2 px-4 rounded\" onclick=\"document.getElementById('development-availability').classList.toggle('hidden')\">Close</button>\n</div>\n\n<div id=\"availability\" class=\"fixed top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2 bg-white rounded shadow-md p-4 w-1/2 hidden animate-fade-in\">\n    <h3 class=\"text-lg font-bold mb-2\">Availability</h3>\n    <p class=\"text-gray-700\">Competitive Team is Currently Filled! We are not accepting new members at this time.</p>\n    <button class=\"bg-orange-500 hover:bg-orange-700 text-white font-bold py-2 px-4 rounded\" onclick=\"document.getElementById('availability').classList.toggle('hidden')\">Close</button>\n</div>\n\n<style>\n    .animate-fade-in {\n        animation: fadeIn 0.5s;\n    }\n\n    @keyframes fadeIn {\n        from {\n            opacity: 0;\n            transform: scale(0.5) translateX(-50%) translateY(-50%);\n        }\n        to {\n            opacity: 1;\n            transform: scale(1) translateX(-50%) translateY(-50%);\n        }\n    }\n</style>\n\n<div id=\"popup-container\" class=\"fixed bottom-0 left-0 p-4 flex space-x-4 z-50 bg-gray-800 text-white rounded shadow-md animate-fade-in\">\n    <p class=\"text-sm\">Welcome to the official John Dewey Mock Trial Team Website! Would you like to enable Mock Mode?</p>\n    <button class=\"bg-orange-500 hover:bg-orange-700 text-white font-bold py-2 px-4 rounded md:py-1 md:px-2\" onclick=\"enableMockMode(); document.getElementById('popup-container').classList.add('hidden')\">Yes</button>\n    <button class=\"bg-orange-500 hover:bg-orange-700 text-white font-bold py-2 px-4 rounded md:py-1 md:px-2\" onclick=\"document.getElementById('popup-container').classList.add('hidden')\">No</button>\n</div>\n\n<script>\n    function enableMockMode() {\n        document.body.classList.add(\"rainbow-text\");\n        document.body.classList.add(\"dark-mode\");\n    }\n</script>\n\n<script>\n    window.onload = function() {\n        document.getElementById('popup-container').classList.remove('hidden');\n    }\n</script>\n\n<section class=\"bg-white py-12\"></section>\n    <div class=\"container mx-auto px-4\" contenteditable=\"true\">\n        <h2 class=\"text-5xl font-bold uppercase text-center mb-4 text-golden\" style=\"text-shadow: 0 0 10px #FFD700;\">Team Of 2025</h2>\n        <div class=\"flex justify-center\">\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Coaches</h3>\n                    <ul>\n                        <li class=\"text-gray-700\">Aleksandr Filipov</li>\n                        <li class=\"text-gray-700\">Sam Callis</li>\n                        <li class=\"text-gray-700\">Danny Farran</li>\n                        <li class=\"text-gray-700\">Mr. Schwartzberg</li><li class=\"text-gray-700\">Test Coach</li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Team Captain</h3>\n                    <ul>\n                        <li class=\"text-gray-700\">Currently being voted on!</li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Members</h3>\n                    <ul>\n                        <li class=\"text-gray-700\">Luiza Hamrakulova /Lawyer</li>\n                        <li class=\"text-gray-700\">Lizi Makashvili /Lawyer</li>\n                        <li class=\"text-gray-700\">Mindy Zou /Expert Witness</li>\n                        <li class=\"text-gray-700\">Amir Faridun Ahmetjanov /Lawyer</li>\n                        <li class=\"text-gray-700\">Helena Calixte /Witness</li>\n                        <li class=\"text-gray-700\">Lila Chen /Witness</li>\n                        <li class=\"text-gray-700\">Melvin Macoto /Witness</li>\n                        <li class=\"text-gray-700\">Sameer Saleemi /Lawyer</li>\n                        <li class=\"text-gray-700\">Zhao Ying Zhen /Witness</li>\n                        <li class=\"text-gray-700\">Eric Huang /Expert Witness</li>\n                        <li class=\"text-gray-700\">Leon Gladyshev /Lawyer</li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"w-full md:w-1/2 xl:w-1/3 p-6\">\n                <div class=\"bg-white rounded shadow-md p-4\">\n                    <h3 class=\"text-lg font-bold mb-2\">Lead Designers</h3>\n                    <ul>\n                        <li class=\"text-gray-700\">Lila Chen</li>\n                    </ul>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <a href=\"https://johndeweyhighschool.org\" target=\"_blank\"><img src=\"school-logo.png\" class=\"fixed top-20 right-4 w-12 h-12 md:w-24 md:h-24\" alt=\"School Logo\" style=\"filter: brightness(1);\"></a>\n\n    <style>\n        /* Styling for the floating edit menu */\n        #edit-menu {\n          position: absolute;\n          background: #fff;\n          border: 1px solid #ccc;\n          padding: 8px;\n          z-index: 1000;\n          display: flex;\n          flex-wrap: wrap;\n          gap: 5px;\n          border-radius: 4px;\n          box-shadow: 0 2px 6px rgba(0,0,0,0.2);\n        }\n        #edit-menu button {\n          background: #3b82f6;\n          color: #fff;\n          border: none;\n          padding: 4px 8px;\n          border-radius: 3px;\n          cursor: pointer;\n          font-size: 0.85rem;\n        }\n        #edit-menu button:hover {\n          background: #2563eb;\n        }\n      </style>\n    \n    \n    \n      <!-- Page Content -->\n\n      <script>\n        document.body.addEventListener('click', function(e) {\n  console.log(\"Body clicked\", e.target);\n  // ... existing code\n});\n\n        </script>\n    \n<div id=\"staff-edit-section\" class=\"p-4 my-4 border-2 border-dashed border-blue-500 rounded\" style=\"display: block;\">\n    <h2 class=\"text-xl font-bold mb-2\">Staff Edit Section</h2>\n    <button onclick=\"saveStaffContent()\" class=\"bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded\">\n      Save All Changes to Server\n    </button>\n  </div>\n\n  <script>\n    // Show the staff edit section if logged in\n    function showStaffEditSection() {\n      if (localStorage.getItem('loggedIn') === 'true') {\n        document.getElementById('staff-edit-section').style.display = 'block';\n      }\n    }\n    document.addEventListener('DOMContentLoaded', function() {\n      showStaffEditSection();\n      if (localStorage.getItem('loggedIn') === 'true') {\n        loadStaffContent();\n        // Make all containers editable\n        const containers = document.querySelectorAll('.container');\n        containers.forEach(container => {\n          container.contentEditable = 'true';\n        });\n      }\n    });\n\n    // Load staff content from the server into the editable area.\n    function loadStaffContent() {\n      fetch('/api/staff-content')\n        .then(response => response.json())\n        .then(data => {\n          if (data.staffContent) {\n            document.body.innerHTML = data.staffContent;\n          }\n        })\n        .catch(error => console.error('Error loading staff content:', error));\n    }\n\n    // Save the content from the editable area to the server.\n    function saveStaffContent() {\n      const contentToSave = document.body.innerHTML;\n      const token = localStorage.getItem('token');\n      fetch('/api/staff-content', {\n        method: 'POST',\n        headers: { \n          'Content-Type': 'application/json',\n          'Authorization': `Bearer ${token}` // In production, verify this token on the server.\n        },\n        body: JSON.stringify({ content: contentToSave })\n      })\n      .then(response => response.json())\n      .then(data => {\n        alert(data.message);\n      })\n      .catch(error => console.error('Error saving staff content:', error));\n    }\n  </script>\n\n   